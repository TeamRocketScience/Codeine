{
    "Info":
    {
        "Name":"Codeine Site",
        "Version":"4.22"
    },
    "Options":
        {
            "TrustHost":    "127.0.0.1",
            "Profiling":    0,
            "MemLeaks":     false,
            "AllowDirectCall": true,
            "FatalCatch":   true,
            "Registration": "Open",
            "Start": "/web/Test/Base"
        },
    "Log":
        {
            "Driver": "FirePHP",
            "Verbose": 9,
            "DSN": ""
        },
    "Drivers":
        {
            "Available":
                {
                    "Data/Lookup":          ["Enum","File","Range"],
                    "Data/Type":            ["String","Integer","Text"],
                    "Output/Elements/Form": ["HTML","Textfield","Select"]
                },
            "Installed":
                {
                    "Input/Filters":      "KillEmAll",
                    "Error/Loggers":      "FirePHP",
                    "System/Profilers":   "Log",
                    "Error/Handlers/404": "404",
                    "Security/CAPTCHA":   "vx7",
                    "Service/IP2Geo":     "IPInfoDB",
                    "Process/Hash":       "sha1",
                    "Output/Serializers": "Codeine",
                    "Output/Elements/Paginators": "Codeine",
                    "Security/User":      ["Native"],
                    "Routers":      ["Native"],
                    "Processors":
                        {
                            "Each":     ["Parslet"],
                            "Post":     ["Parslet", "MediaTag2","Ico2","AjaxSlices","JSRun","CSSRun", "I18N"],
                            "Fusion":   ["Const","Foreach","Authorized","Owner","Role","Node","Tag","Key","For"]
                        }
                }
        },
    "Hooks":
        {
            "Client":
                {
                    "afterInitialize": ["Locale","Online","TrustIP"]
                },
            "Server":
                {
                    "afterInitialize": ["Sanitarize","MBString"]
                },
            "View":
                {
                    "beforeOutput": ["Perfomance"],
                    "beforeLoad": ["Authorized","Role"]
                },
            "User":
                {
                    "beforeCreate": ["SelfOwner","AutoAuth"]
                }
        },
    "Engines":
        {
            "Application":    "Sigma",
            "Access":         "Piranha",
            "Client":         "Nebula",
            "Code":           "M60",
            "Collection":     "Palladium",
            "Data":           "E9",
            "Event":          "Eve",
            "Form":           "Formalin",
            "Log":            "Wolf",
            "View":           "D8",
            "Object":         "Iridium",
            "Server":         "Spark",
            "Profiler":       "Sentinel",
            "Message":        "RedCoast"
        },
   "Shared":
        {
            "Engine": "/var/www/Codeine/Default"
        },
   "Paths":
        {
            "Core":           "Core",
            "Package":        "Package",
            "Driver":         "Driver",
            "Apps":           "Application",
            "Fonts":          "Fonts",
            "Model":          "Model",
            "Icons":          "Images/Icons",
            "Images":         "Images",
            "Static":         "Static",
            "Styles":         "Styles",
            "Layout":         "Layout",
            "Data":           "Data",
            "Temp":           "Temp",
            "Flash":          "Fl",
            "JS":             "JS",
            "CSS":            "CSS"
        }, 
    "Storages":
        {
            "SQL3D":    {
                "Method": "OOMySQLi",
                "ORM": "SQL3D",
                "User": "codeine",
                "Password":"codeine",
                "Host":"localhost",
                "Port":"3306",
                "Database": "codeine"
                        },
            "FS":       { "Method": "SubFS",     "ORM": "FS",        "DSN": "/"},
            "Layout":   { "Method": "LayoutFS",  "ORM": "FS",        "DSN": "/"},
            "XML":      { "Method": "SubXMLFS",  "ORM": "FS",        "DSN": "/"},
            "Model":    { "Method": "ModelFS",   "ORM": "FS",        "DSN": "/"},
            "JSON":     { "Method": "SubJSONFS", "ORM": "FS",        "DSN": "/"},
            "Cookie":   { "Method": "Cookie",    "ORM": "NativeKVS"},
            "HTTP":     { "Method": "HTTP",      "ORM": "NativeKVS"},
            "Static":   { "Method": "Static",    "ORM": "NativeKVS"},
            "Default":  { "Method": "Static",    "ORM": "NativeKVS"}
        },
    "Mounts":
        {
            "Default":       {"ReadCache:TTL": 600},
            "Application":   {"Node": "SQL3D",  "Dir": "_Application",  "ReadCache": "Static"},
            "Access":        {"Node": "SQL3D",  "Dir": "_Access"},
            "User":          {"Node": "SQL3D",  "Dir": "User",         "ReadCache": "Static"},
            "Ticket":        {"Node": "SQL3D",  "Dir": "_Ticket",       "ReadCache": "Static"},
            "IP2Geo":        {"Node": "SQL2D",  "Dir": "_IP2Geo",       "ReadCache": "Static"},
            "Route":         {"Node": "SQL3D",  "Dir": "_Route",        "ReadCache": "Static"},
            "Signal":        {"Node": "SQL3D",  "Dir": "_Signal"},
            "Language":      {"Node": "SQL3D",  "Dir": "_Language"},
            "Node":          {"Node": "SQL3D",  "Dir": "_Node"},
            "Slot":          {"Node": "SQL3D",  "Dir": "_Slot"},
            "Command":       {"Node": "SQL3D",  "Dir": "_Command"},
            "Info":          {"Node": "SQL3D",  "Dir": "Info"},
            "Layout":        {"Node": "Layout", "Dir": "Layout",        "ReadCache": "Static"},
            "UserFS":        {"Node": "FS",     "Dir": "Data"},
            "JSONFS":        {"Node": "JSON",   "Dir": "Data"},
            "Static":        {"Node": "JSON",   "Dir": "Static"},
            "Model":         {"Node": "Model",  "Dir": "Model"},
            "Cookie":        {"Node": "Cookie", "Dir": "Cookie"},
            "HTTP":          {"Node": "HTTP",   "Dir": "HTTP"}
        },
    "Sensors":
        {
            "Timing":
                {
                    "Root": {"Min": 0, "Max": 100},
                    "Code:Data/Mounters%": {"Min":0,"Max":100}
                }
        }
}