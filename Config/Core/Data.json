{
    "Routers": ["PointID"],
    "Stores":
    {
        "MySQL":
        {
            "Type": "SQL/MySQL",
            "Username": "root",
            "Password":"phoenix",
            "Server":"localhost",
            "Port":"3306",
            "Database": "Codeine",
            "Charset": "UTF-8"
        },
        "Static":
        {
            "Type": "PHP/Static",
            "Map": "Native"
        },
        "Layout":
        {
            "Type": "File/Layout",
            "DSN": "Layout"
        },
        "FS":
        {
            "Type": "File/Cascade",
            "DSN": ""
        },
        "Temp":
        {
            "Type": "File/Flat",
            "DSN": "/Temp"
        },
        "Redis":
        {
            "Type": "KeyValue/Redis"
        }
    },
    "Points":
    {
        "Default": {"Store": "Static", "Map": "Native", "Scope": "Default"},

        "Contract":{
            "Store": "FS",
            "Format": "JSON",
            "Scope": "Config/Driver",
            "Postfix": ".json",
            "Ring": 1},

        "Model":{
            "Store": "FS",
            "Format": "JSON",
            "Scope": "Config/Model",
            "Postfix": ".json",
            "Ring": 1},

        "Locale":{
            "Store": "FS",
            "Format": "JSON",
            "Scope": "Data/Locale",
            "Postfix": ".json"},

        "Test":    {"Store": "MySQL",  "Map": "Fluid",  "Scope": "Test"},

        "Layout":  {"Store": "Layout", "Map": "File",   "Scope": "Layout"},
        "FS":      {"Store": "FS",     "Map": "File",   "Scope": ""},
        "JS":      {"Store": "FS",     "Map": "File",   "Scope": "Default/JS"},
        "CSS":     {"Store": "FS",     "Map": "File",   "Scope": "Default/CSS"},
        "TempCSS": {"Store": "Temp",   "Map": "File",   "Scope": "CSS"},
        "TempJS":  {"Store": "Temp",   "Map": "File",   "Scope": "JS"},
        "Redis":   {"Store": "Redis",  "Map": "KeyValue", "Scope": "Redis"},
        "CodeCache": {"Store": "Redis", "Map": "KeyValue", "Scope": "Code"}
    },
    "Shared":
    {
        "Engine": "/var/www/Codeine/Default"
    },
   "Includes":
    {
        "Zend": "/usr/share/php/libzend-framework-php/"
    },
   "Paths":
    {
        "Code":           "Driver",
        "Package":        "Package",
        "Fonts":          "Fonts",
        "Model":          "Model",
        "Icons":          "Images/Icons",
        "Images":         "Images",
        "Static":         "Static",
        "Styles":         "Styles",
        "Layout":         "Layout",
        "Data":           "Data",
        "Temp":           "Temp",
        "Flash":          "Fl",
        "JS":             "JS",
        "CSS":            "CSS"
    }
}